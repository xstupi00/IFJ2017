' factorial iterative
scope 'Hlavni telo programu
	Dim a As Integer
	DIM vysl AS INTEGER
	PrinT !"Zadejte cislo pro vypocet faktorialu";
	InpuT A
	If a < 0 THEN
		print !"\nFaktorial nelze spocitat\n";
	ELSE
		Vysl = 1
	Do WHile A > 0
		VYSL = vysl * a
		a = A - 1
	LooP
	Print !"\nVysledek je:" ; vYsl ; !"\n";
	end IF
END SCOPE
